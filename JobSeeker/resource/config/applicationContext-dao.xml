<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
           http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
           http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd"
	default-autowire="byName" default-lazy-init="true">
	<bean id="userDAO" class="com.JobSeeker.dao.UserDAOImpl" >
	    <constructor-arg>
		<ref bean="hibernateTemplate" />
		</constructor-arg>
	</bean>
	<bean id="jobDAO" class="com.JobSeeker.dao.JobDAOImpl" >
	    <constructor-arg>
		<ref bean="hibernateTemplate" />
		</constructor-arg>
	</bean>
	<bean id="usersAndJobsDAO" class="com.JobSeeker.dao.UsersAndJobsDAOImpl" >
	    <constructor-arg>
		<ref bean="hibernateTemplate" />
		</constructor-arg>
	</bean>
	<bean id="feedDAO" class="com.JobSeeker.dao.FeedDAOImpl" >
	    <constructor-arg>
		<ref bean="hibernateTemplate" />
		</constructor-arg>
	</bean>
</beans>
